{% extends 'AppUserBundle:Admin:layout.html.twig' %}

{% block fos_user_content %}
{% form_theme form theme %}

<form action="{{ path('app_user_admin_user_edit', {'id' : kit_user_id}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_profile_edit">
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'profile.edit.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>
{% endblock fos_user_content %}
